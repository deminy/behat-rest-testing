{
    "name": "deminy/behat-rest-testing",
    "description": "Demo on doing feature tests with Behat 3 and how to start writing feature tests for REST APIs.",
    "keywords": [
        "PHP",
        "Behat",
        "REST"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Demin Yin",
            "email": "deminy@deminy.net",
            "homepage": "http://deminy.net"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "behat/behat": "~3.0.14",
        "behat/webApiExtension" : "dev-master",
        "phpunit/phpunit": ">=4.0.0,<4.6.0"
    },
    "autoload" : {
        "psr-4" : {
            "Behat\\RestTestingContext\\" : "src/RestTestingContext/",
            "Behat\\RestTestingExtension\\" : "src/RestTestingExtension/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
